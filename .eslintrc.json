{
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-unused-vars": 0,
    "semi": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "react": true
  },
  "extends": "eslint:recommended"
}
